@model List<libarySystem.ViewModels.QuerryLendRecord>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{

}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table class="table table-striped table-hover">
        <thead>
            <tr>
               
                <th scope="col">借閱日期</th>
                <th scope="col">借閱人員編號</th>
                <th scope="col">英文姓名</th>
                <th scope="col">中文姓名</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                    <td>@item.LEND_DATE</td>
                    <td>@item.KEEPER_ID</td>
                    <td>@item.USER_ENAME</td>
                    <td>@item.USER_CNAME</td>
            </tr>
       
        }
        </tbody>
    </table>
</body>
</html>